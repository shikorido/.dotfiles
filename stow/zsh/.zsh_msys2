# Windows ssh works better.
alias ssh=/c/Windows/System32/OpenSSH/ssh.exe

#addToPathFront /clang64/bin
# We can take win32yank.exe from Neovim for Windows
# and place it somewhere to use convenient yanking.
alias nvim=/clang64/bin/nvim
alias nvim.exe=/clang64/bin/nvim
alias nvimqt=/clang64/bin/nvim-qt
alias nvimqt.exe=/clang64/bin/nvim-qt

